{
  "name": "react-starter",
  "author": "Paralect",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paralect/koa-react-starter.git"
  },
  "keywords": [
    "react",
    "redux",
    "paralect"
  ],
  "engines": {
    "node": ">= 14"
  },
  "scripts": {
    "start": "webpack-dev-server --color --config webpack.dev.config.js",
    "test": "run-s test:**",
    "test:eslint": "eslint --ext js --ext jsx ./src",
    "test:stylelint": "stylelint \"src/**/*.pcss\"",
    "build": "webpack -p"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --ext js --ext jsx --fix"
    ],
    "*.pcss": [
      "stylelint --fix"
    ]
  },
  "dependencies": {
    "axios": "0.21.1",
    "classnames": "2.2.6",
    "core-js": "3.6.5",
    "history": "4.10.1",
    "lodash": "4.17.20",
    "prop-types": "15.7.2",
    "qs": "6.9.4",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-redux": "7.2.1",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "react-toast-notifications": "2.4.0",
    "redux": "4.0.5",
    "redux-thunk": "2.3.0",
    "regenerator-runtime": "0.13.7",
    "socket.io-client": "2.3.1",
    "webpack": "4.43.0",
    "yup": "0.29.3"
  },
  "devDependencies": {
    "@babel/cli": "7.12.1",
    "@babel/core": "7.12.3",
    "@babel/eslint-parser": "7.12.1",
    "@babel/preset-env": "7.12.1",
    "@babel/preset-react": "7.12.1",
    "@pmmmwh/react-refresh-webpack-plugin": "0.4.2",
    "@svgr/webpack": "5.4.0",
    "babel-loader": "8.1.0",
    "babel-plugin-module-resolver": "4.0.0",
    "copy-webpack-plugin": "6.2.1",
    "css-loader": "4.3.0",
    "cssnano": "4.1.10",
    "eslint": "7.11.0",
    "eslint-config-airbnb": "18.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-react": "7.21.4",
    "eslint-plugin-react-hooks": "4.1.2",
    "html-webpack-plugin": "4.5.0",
    "husky": "4.3.0",
    "lint-staged": "10.4.2",
    "mini-css-extract-plugin": "1.1.0",
    "normalize.css": "8.0.1",
    "npm-run-all": "4.1.5",
    "postcss-import": "12.0.1",
    "postcss-loader": "3.0.0",
    "postcss-nested": "4.2.1",
    "postcss-preset-env": "6.7.0",
    "react-refresh": "0.8.3",
    "style-loader": "1.2.1",
    "stylelint": "13.7.2",
    "stylelint-config-standard": "20.0.0",
    "url-loader": "4.1.1",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0"
  }
}
